<?xml version="1.0" encoding="utf-8"?>
<xsd:schema targetNamespace="http://www.ed.gov/FSA/COD/2023/v5.0b" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:cod="http://www.ed.gov/FSA/COD/2023/v5.0b" elementFormDefault="qualified">
	<xsd:element name="CommonRecord" type="cod:CommonRecordType">
		<xsd:annotation>
			<xsd:documentation>Root Node for Department of Education Common Record</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="CommonRecordType">
		<xsd:sequence>
			<xsd:element name="TransmissionData">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="DocumentID">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="30"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="CreatedDateTime" type="xsd:dateTime"/>
						<xsd:element name="Source" type="cod:SourceType"/>
						<xsd:element name="Destination" type="cod:DestinationType"/>
						<xsd:element name="Software" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="SoftwareProvider" minOccurs="0">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="10"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="SoftwareVersion" minOccurs="0">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="6"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="OtherSoftwareProvider" nillable="true" minOccurs="0">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="10"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="OtherSoftwareVersion" nillable="true" minOccurs="0">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="6"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="FullResponseCode" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:length value="1"/>
									<xsd:enumeration value="S"/>
									<xsd:enumeration value="F"/>
									<xsd:enumeration value="M"/>
									<xsd:enumeration value="N"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="ReportingSchool" type="cod:ReportingSchoolType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="Receipt" type="xsd:dateTime" minOccurs="0"/>
			<xsd:element name="Response" type="cod:CommonRecordResponseType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CommonRecordResponseType">
		<xsd:sequence>
			<xsd:element name="DocumentTypeCode" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:length value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DocumentStatusCode" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="1"/>
						<xsd:enumeration value="A"/>
						<xsd:enumeration value="R"/>
						<xsd:enumeration value="D"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ProcessDate" type="xsd:date" minOccurs="0"/>
			<xsd:element name="EditProcessResult" type="cod:EditProcessResultType" minOccurs="0" maxOccurs="100"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="EntityRefType">
		<xsd:sequence>
			<xsd:element name="RoutingID">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="8"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="AttendedSchoolType">
		<xsd:complexContent>
			<xsd:extension base="cod:EntityRefType">
				<xsd:sequence>
					<xsd:element name="Student" type="cod:StudentType" minOccurs="0" maxOccurs="unbounded">
						<xsd:key name="LIK">
							<xsd:selector xpath="LoanInformation"/>
							<xsd:field xpath="@AwardKey"/>
						</xsd:key>
						<xsd:keyref name="LIKRef" refer="cod:LIK">
							<xsd:selector xpath="FinancialAward"/>
							<xsd:field xpath="AwardKey"/>
						</xsd:keyref>
					</xsd:element>
					<xsd:element name="Response" type="cod:ResponseType" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ReportingSchoolType">
		<xsd:complexContent>
			<xsd:extension base="cod:EntityRefType">
				<xsd:sequence>
					<xsd:element name="ReportedFinancialSummary" type="cod:ReportedFinancialSummaryType" minOccurs="0" maxOccurs="120"/>
					<xsd:element name="AttendedSchool" type="cod:AttendedSchoolType" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element name="Response" type="cod:ReportingSchoolResponseType" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ReportedFinancialSummaryType">
		<xsd:sequence>
			<xsd:element name="FinancialAwardType" type="cod:FinancialAwards"/>
			<xsd:element name="FinancialAwardYear" type="xsd:gYear"/>
			<xsd:element name="TotalCount" type="cod:TotalCountType"/>
			<xsd:element name="TotalReportedAward" type="cod:LargeCurrencyType" minOccurs="0"/>
			<xsd:element name="TotalReportedDisbursement" type="cod:LargeCurrencyType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DestinationType">
		<xsd:choice>
			<xsd:element name="School" type="cod:EntityRefType"/>
			<xsd:element name="ThirdPartyServicer" type="cod:EntityRefType"/>
			<xsd:element name="Guarantor" type="cod:EntityRefType"/>
			<xsd:element name="Lender" type="cod:EntityRefType"/>
			<xsd:element name="Other" type="cod:EntityRefType"/>
			<xsd:element name="COD" type="cod:EntityRefType"/>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="SourceType">
		<xsd:choice>
			<xsd:element name="School" type="cod:EntityRefType"/>
			<xsd:element name="ThirdPartyServicer" type="cod:EntityRefType"/>
			<xsd:element name="Guarantor" type="cod:EntityRefType"/>
			<xsd:element name="Lender" type="cod:EntityRefType"/>
			<xsd:element name="Other" type="cod:EntityRefType"/>
			<xsd:element name="COD" type="cod:EntityRefType"/>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="PersonType">
		<xsd:sequence>
			<xsd:element name="Index" type="cod:IndexType" />
			<xsd:element name="PersonIdentifiers" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="SSN" type="cod:SSNType" minOccurs="0"/>
						<xsd:element name="DriversLicense" type="cod:DriversLicenseType" minOccurs="0"/>
						<xsd:element name="SchoolAssignedPersonID" nillable="true" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="20"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Birth" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="BirthDate" type="xsd:date" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Name" type="cod:NameType" minOccurs="0"/>
			<xsd:element name="Contacts" type="cod:ContactsType" minOccurs="0"/>
			<xsd:element name="Citizenship" type="cod:CitizenshipType" minOccurs="0"/>
			<xsd:element name="Note" type="cod:SchoolNoteMessageType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BorrowerType">
		<xsd:complexContent>
			<xsd:extension base="cod:PersonType">
				<xsd:sequence>
					<xsd:element name="DefaultOverpayCode" type="cod:DefaultOverpayCodeType" minOccurs="0"/>
					<xsd:element name="Response" type="cod:ResponseType" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="StudentType">
		<xsd:complexContent>
			<xsd:extension base="cod:PersonType">
				<xsd:sequence>
					<xsd:element ref="cod:LoanInformation" minOccurs="0" maxOccurs="60"/>
					<xsd:element ref="cod:FinancialAward" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="cod:Counseling" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element name="Response" type="cod:StudentResponseType" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="EndorserType">
		<xsd:sequence>
			<xsd:element name="Index" type="cod:IndexType" />
			<xsd:element name="Name" type="cod:EndorserNameType" minOccurs="0"/>
			<xsd:element name="DriversLicense" type="cod:DriversLicenseType" minOccurs="0"/>
			<xsd:element name="Contacts" type="cod:ContactsType" minOccurs="0"/>
			<xsd:element name="Citizenship" type="cod:CitizenshipType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:element name="LoanInformation" type="cod:LoanInformationType" abstract="true"/>
	<xsd:element name="DLLoanInformation" type="cod:DLLoanInformationType" substitutionGroup="cod:LoanInformation"/>
	<xsd:complexType name="LoanInformationType">
		<xsd:sequence>
			<xsd:element name="OriginationFeePercent" nillable="true" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="100"/>
						<xsd:totalDigits value="6"/>
						<xsd:fractionDigits value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="InterestRebatePercent" nillable="true" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="100"/>
						<xsd:totalDigits value="6"/>
						<xsd:fractionDigits value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PromissoryNotePrintCode" nillable="true" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:length value="1"/>
						<xsd:enumeration value="S"/>
						<xsd:enumeration value="R"/>
						<xsd:enumeration value="Z"/>
						<xsd:enumeration value="V"/>
						<xsd:enumeration value="O"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DisclosureStatementPrintCode" nillable="true" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:length value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="StudentLevelCode" type="cod:StudentLevelCode" nillable="true" minOccurs="0"/>
			<xsd:element name="FinancialAwardBeginDate" type="xsd:date" nillable="true" minOccurs="0"/>
			<xsd:element name="FinancialAwardEndDate" type="xsd:date" nillable="true" minOccurs="0"/>
			<xsd:element name="AcademicYearBeginDate" type="xsd:date" nillable="true" minOccurs="0"/>
			<xsd:element name="AcademicYearEndDate" type="xsd:date" nillable="true" minOccurs="0"/>
			<xsd:element name="PublishedPgmLengthYears" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="999.999"/>
						<xsd:totalDigits value="6"/>
						<xsd:fractionDigits value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PublishedPgmLengthMonths" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="999.999"/>
						<xsd:totalDigits value="6"/>
						<xsd:fractionDigits value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PublishedPgmLengthWeeks" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="999.999"/>
						<xsd:totalDigits value="6"/>
						<xsd:fractionDigits value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="WeeksProgramsAcademicYear" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="999.999"/>
						<xsd:totalDigits value="6"/>
						<xsd:fractionDigits value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="SpecialPrograms" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:length value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ProgramCredentialLevel" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="0"/>
						<xsd:maxInclusive value="99"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="AwardKey" type="xsd:integer" use="required"/>
	</xsd:complexType>
	<xsd:complexType name="DLLoanInformationType">
		<xsd:complexContent>
			<xsd:extension base="cod:LoanInformationType">
				<xsd:sequence>
					<xsd:element name="Response" type="cod:ResponseType" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="FinancialAward" type="cod:FinancialAwardType" abstract="true">
		<xsd:annotation>
			<xsd:documentation>At this location in your document you can include one or more FinancialAwardType elements (ex: DLPLUS, DLSubsidized, Pell, DLUnsubsidized, and TEACH)  </xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="Pell" type="cod:PellType" substitutionGroup="cod:FinancialAward"/>
	<xsd:element name="DLSubsidized" type="cod:DLSubsidizedType" substitutionGroup="cod:FinancialAward"/>
	<xsd:element name="DLUnsubsidized" type="cod:DLUnsubsidizedType" substitutionGroup="cod:FinancialAward"/>
	<xsd:element name="DLPLUS" type="cod:DLPLUSType" substitutionGroup="cod:FinancialAward"/>
	<xsd:element name="TEACH" type="cod:TEACHType" substitutionGroup="cod:FinancialAward"/>
	<xsd:element name="Counseling" type="cod:CounselingType"/>
	<xsd:complexType name="FinancialAwardType">
		<xsd:sequence>
			<xsd:element name="AwardKey" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="FinancialAwardYear" type="xsd:gYear"/>
			<xsd:element name="FPSTransactionNumber" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:integer">
						<xsd:minInclusive value="1"/>
						<xsd:maxInclusive value="99"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="FinancialAwardAmount" type="cod:SmallCurrencyType" nillable="true" minOccurs="0"/>
			<xsd:element name="Note" type="cod:SchoolNoteMessageType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="LoanAwardType">
		<xsd:complexContent>
			<xsd:extension base="cod:FinancialAwardType">
				<xsd:sequence>
					<xsd:element name="DependencyStatusCode" nillable="true" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="I"/>
								<xsd:enumeration value="D"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="EntranceCounseling" type="xsd:boolean" nillable="true" minOccurs="0"/>
					<xsd:element name="DefaultOverpayCode" type="cod:DefaultOverpayCodeType" minOccurs="0"/>
					<xsd:element name="FinancialAwardNumber" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:integer">
								<xsd:minInclusive value="1"/>
								<xsd:maxInclusive value="999"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="FinancialAwardID" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="21"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="DirectLoanAttendanceCost" type="cod:TotalCountType" nillable="true" minOccurs="0" />
					<xsd:element name="FinancialAwardCreateDate" type="xsd:date" nillable="true" minOccurs="0"/>
					<xsd:element name="ProgramOfStudy" nillable="true" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="3"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="StudentEligibilityCode" nillable="true" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="2"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="AbilityToBenefitTestAdministratorCode" nillable="true" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="2"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="AbilityToBenefitTestCode" nillable="true" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="2"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="AbilityToBenefitStateCode" type="cod:StateProvinceCodeType" nillable="true" minOccurs="0"/>
					<xsd:element name="AbilityToBenefitCompletionDate" type="xsd:date" nillable="true" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="PellType">
		<xsd:complexContent>
			<xsd:extension base="cod:FinancialAwardType">
				<xsd:sequence>
					<xsd:element name="FinancialAwardNumber" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:integer">
								<xsd:minInclusive value="1"/>
								<xsd:maxInclusive value="999"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="FinancialAwardID" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="21"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="AttendanceCost" type="cod:SmallCurrencyType" nillable="true" minOccurs="0"/>
					<xsd:element name="LowTuitionFeesCode" nillable="true" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:length value="1"/>
								<xsd:enumeration value="1"/>
								<xsd:enumeration value="2"/>
								<xsd:enumeration value="3"/>
								<xsd:enumeration value="4"/>
								<xsd:enumeration value=" "/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="IncarceratedIndicator" type="xsd:boolean" nillable="true" minOccurs="0"/>
					<xsd:element name="VerificationStatusCode" nillable="true" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="1"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="EnrollmentDate" type="xsd:date" nillable="true" minOccurs="0"/>
					<xsd:element name="AdditionalEligibilityIndicator" type="xsd:boolean" minOccurs="0"/>
					<xsd:element name="StudentEligibilityCode" nillable="true" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="2"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="AbilityToBenefitTestAdministratorCode" nillable="true" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="2"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="AbilityToBenefitTestCode" nillable="true" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="2"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="AbilityToBenefitStateCode" type="cod:StateProvinceCodeType" nillable="true" minOccurs="0"/>
					<xsd:element name="AbilityToBenefitCompletionDate" type="xsd:date" nillable="true" minOccurs="0"/>
					<xsd:element name="Response" type="cod:GrantAwardResponseType" minOccurs="0"/>
					<xsd:element name="Disbursement" type="cod:GrantDisbursementType" minOccurs="0" maxOccurs="2000"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="PLUSAwardType">
		<xsd:complexContent>
			<xsd:extension base="cod:LoanAwardType">
				<xsd:sequence>
					<xsd:element name="FinancialAwardAmountRequested" type="cod:SmallCurrencyType" nillable="true" minOccurs="0"/>
					<xsd:element name="ApplicationID" minOccurs="0">
						<xsd:simpleType>
							<xsd:restriction base="xsd:integer">
								<xsd:maxInclusive value="9999999999"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="Borrower" type="cod:BorrowerType"/>					
					<xsd:element name="Response" type="cod:PLUSAwardResponseType" minOccurs="0"/>
					<xsd:element name="Disbursement" type="cod:DLDisbursementType" minOccurs="0" maxOccurs="400"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
