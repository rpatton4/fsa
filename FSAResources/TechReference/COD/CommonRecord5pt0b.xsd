                                <xsd:enumeration value="S"/>
                                <xsd:enumeration value="O"/>
                            </xsd:restriction>
                        </xsd:simpleType>
                    </xsd:element>					
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
